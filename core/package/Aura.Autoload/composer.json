{
    "name": "aura/autoload",
    "type": "aura-package",
    "description": "The Aura Autoload package provides a PSR-0 compliant SPL autoloader\nimplementation for PHP. It also matches the proposed SplClassLoader interface.\n\n[PSR-0]: https://github.com/php-fig/fig-standards/blob/master/accepted/PSR-0.md\n[SplClassLoader]: https://wiki.php.net/rfc/splclassloader",
    "keywords": [
        "PSR-0",
        "autoload",
        "autoloader",
        "SPL autoloader",
        "SplClassLoader",
        "class loader"
    ],
    "homepage": "http://auraphp.github.com/Aura.Autoload",
    "time": "2013-04-12",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Aura.Autoload Contributors",
            "homepage": "https://github.com/auraphp/Aura.Autoload/contributors"
        },
        {
            "name": "Paul M. Jones",
            "email": "pmjones88@gmail.com",
            "homepage": "http://paul-m-jones.com"
        },
        {
            "name": "Hari KT",
            "email": "kthari85@gmail.com",
            "homepage": "http://harikt.com"
        },
        {
            "name": "Akihito Koriyama",
            "email": "akihito.koriyama@gmail.com",
            "homepage": null
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "aura/installer-default": ">=1.0.0"
    },
    "autoload": {
        "psr-0": {
            "Aura\\Autoload": "src/"
        }
    }
}